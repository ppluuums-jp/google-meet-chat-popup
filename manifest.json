{
  "name": "Google Meet Chat Popup",
  "description": "You can see the chat no matter which tab you are on.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions":[
    "contextMenus",
    "tabs",
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions":[
    "https://meet.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"}
  }
